<Layout>
  <div v-slot:foo="aaa">{{aaa.b}}</div>
</Layout>
var vnode = new VNode({
  name: 'Layout',
  scopedSlots: {
    foo: function(aaa) {
      ...
    }
  }
})


### Layout
<div>
  <slot name="foo" a="123" b="456">
</div>
var props = {a: 123,b: 456}
var func = this.$scopedSlots['foo']
func(props)